{
	"name": "@niku/vue3-form",
	"version": "1.0.0",
	"main": "dist/index.cjs.js",
	"module": "dist/index.es.js",
	"types": "dist",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build && yarn compile-style",
		"compile-style": "sass --no-source-map ./src/styles/index.scss ./dist/styles/index.css && cp ./src/styles/index.scss ./dist/styles/index.scss",
		"preview": "vite preview",
		"example": "yarn --cwd example",
		"postinstall": "yarn link && yarn example && yarn example link @niku/vue3-form"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"async-validator": "^4.2.5"
	},
	"peerDependencies": {
		"vue": "^3.2.41"
	},
	"devDependencies": {
		"@types/jsdom": "^20.0.0",
		"@types/node": "^16.11.27",
		"@vitejs/plugin-vue": "^3.1.2",
		"@vue/tsconfig": "^0.1.3",
		"sass": "^1.55.0",
		"typescript": "^4.8.4",
		"vite": "^3.1.8",
		"vite-plugin-dts": "^1.6.6",
		"vue": "^3.2.41",
		"vue-tsc": "^1.0.8"
	}
}
